d page.\n * @param date\n *  the date for which the user wants to view reservations.\n * @returns {JSX.Element}\n */\nfunction Dashboard({ date }) {\n  const [reservations, setReservations] = useState([]);\n  const [reservationsError, setReservationsError] = useState();\n  const [thisDate, setThisDate] = useState(date);\n  const handleNext = () => {\n    setThisDate(next(thisDate));\n  };\n  const handlePrevious = () => {\n    setThisDate(previous(thisDate));\n  };\n  const handleToday = () => {\n    setThisDate(today(thisDate));\n  };\n  useEffect(loadDashboard, [thisDate]);\n  date = thisDate;\n  function loadDashboard() {\n    const abortController = new AbortController();\n    setReservationsError();\n    listReservations({ date }, abortController.signal)\n      .then(setReservations)\n      .catch(setReservationsError);\n    return () => abortController.abort();\n  }\n  return (\n    <main>\n      <h1>Dashboard</h1>\n      <div className=\"d-md-flex mb-3\">\n        <h4 className=\"mb-0\">Reservations for date: {date}</h4>\n      </div>\n      {ErrorAlert(reservationsError)}\n      {reservations.map((reservation) => {\n        return (\n          <div className=\"card mt-2 pt-3\">\n            <div className=\"card-body\">\n              <h3 class=\"d-flex m-3\">\n                {reservation.first_name} {reservation.last_name}\n              </h3>\n              <div class=\"d-flex\">\n                <p class=\"d-flex m-3\">Time: {reservation.reservation_time}</p>\n                <p class=\"d-flex m-3\">People: {reservation.people}</p>\n              </div>\n              <div class=\"d-flex\">\n                <p class=\"d-flex m-3\">Phone #: {reservation.mobile_number}</p>\n                <p class=\"d-flex m-3\">Date: {reservation.reservation_date}</p>\n              </div>\n            </div>\n            <div class=\" p-3 d-flex justify-content-end align-items-center\">\n              <Link\n                to={`/reservations/${reservation.reservation_id}/edit`}\n                type=\"button\"\n                className=\"btn btn-secondary mr-2 btn-md\"\n              >\n                Edit\n              </Link>\n            </div>\n          </div>\n        );\n      })}\n      <div class=\" p-3 d-flex justify-content-end align-items-center\">\n        <button\n          class=\"btn btn-large btn-success\"\n          onClick={() => handlePrevious()}\n        >\n          Previous\n        </button>\n        <button\n          class=\"btn btn-large btn-success m-2\"\n          onClick={() => handleToday()}\n        >\n          Today\n        </button>\n        <button class=\"btn btn-large btn-success\" onClick={() => handleNext()}>\n          Next\n        </button>\n      </div>\n    </main>\n  );\n}\nexport default Dashboard;\n","/Users/david/workspace/finalCapstone/starter-restaurant-reservation/front-end/src/utils/date-time.js",[],"/Users/david/workspace/finalCapstone/starter-restaurant-reservation/front-end/src/utils/api.js",[],"/Users/david/workspace/finalCapstone/starter-restaurant-reservation/front-end/src/layout/ErrorAlert.js",[],"/Users/david/workspace/finalCapstone/starter-restaurant-reservation/front-end/src/utils/format-reservation-date.js",[],"/Users/david/workspace/finalCapstone/starter-restaurant-reservation/front-end/src/layout/NewReservation.js",[],"/Users/david/workspace/finalCapstone/starter-restaurant-reservation/front-end/src/layout/ErrorDisplay.js",[],["72","73"],"/Users/david/workspace/finalCapstone/starter-restaurant-reservation/front-end/src/utils/useQuery.js",[],"/Users/david/workspace/finalCapstone/starter-restaurant-reservation/front-end/src/layout/HandleErrors.js",[],"/Users/david/workspace/finalCapstone/starter-restaurant-reservation/front-end/src/layout/NewTable.js",["74"],{"ruleId":"75","replacedBy":"76"},{"ruleId":"77","replacedBy":"78"},{"ruleId":"79","severity":1,"message":"80","line":26,"column":28,"nodeType":"81","endLine":26,"endColumn":38,"suggestions":"82"},{"ruleId":"